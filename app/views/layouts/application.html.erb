<!DOCTYPE html>
<html>
<head>
  <title>ZillowClone</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if flash[:errors] %>
  <p>
  <%   flash.errors.each do |error| %>
  <%=    error %>
  <%   end %>
  </p>
  <% end %>
<%= yield %>

</body>
</html>
